{
  "name": "empiremud-maps",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "extract:terrain": "node tools/extract-terrain.mjs",
    "fetch:map": "node tools/fetch-map.mjs",
    "clean": "rm -rf dist",
    "generate:textures": "tsx tools/generate-textures.ts --preset low --no-ktx2",
    "generate:textures:high": "tsx tools/generate-textures.ts --preset high --no-ktx2",
    "generate:textures:high-plus": "tsx tools/generate-textures.ts --preset high-plus --no-ktx2",
    "generate:textures:low:compression": "tsx tools/generate-textures.ts --preset low",
    "generate:textures:high:compression": "tsx tools/generate-textures.ts --preset high",
    "generate:textures:full": "tsx tools/generate-textures.ts --preset high-plus",
    "build": "vite build",
    "preview": "vite preview",
    "dev": "vite --host --open",
    "dev:standard": "npm run dev"
  },
  "dependencies": {
    "three": "^0.167.1"
  },
  "devDependencies": {
    "@types/node": "^22.7.9",
    "@types/three": "^0.167.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.6.3",
    "vite": "^7.2.6"
  }
}
